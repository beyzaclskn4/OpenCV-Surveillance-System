   <string notr="true">/* --- GENEL AYARLAR --- */
QMainWindow, QWidget {
    background-color: #1e1e1e; /* Kömür Grisi Arka Plan */
    color: white;
}

/* --- GRUP KUTUSU (Çerçeve) --- */
QGroupBox {
    border: 2px solid #00aaff; /* Neon Mavi Çerçeve */
    border-radius: 6px;
    margin-top: 20px;
    font-weight: bold;
    color: #00aaff;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 5px;
}

/* --- BUTONLAR (Mavi ve Sığacak Şekilde) --- */
QPushButton {
    background-color: #005bb5;
    border: 1px solid #004c99;
    color: white;
    padding: 4px; /* Yazılar sığsın diye boşluğu azalttık */
    border-radius: 5px;
    font-size: 10px;
    font-weight: bold;
    min-height: 25px;
}

QPushButton:hover {
    background-color: #0077ea; /* Parlak Mavi */
    border: 1px solid #00aaff;
}

QPushButton:pressed {
    background-color: #003366; /* Koyu Mavi */
    border: 1px solid #002244;
}

/* --- SLIDER ÖZEL TASARIMI (MAVİ) --- */

/* 1. Kanal (Slider'ın üzerinde gittiği yol) */
QSlider::groove:horizontal {
    border: 1px solid #333;
    height: 8px; /* Yolun kalınlığı */
    background: #222; /* Yolun rengi (Koyu) */
    margin: 2px 0;
    border-radius: 4px;
}

/* 2. Topuz (Tuttuğumuz kısım) */
QSlider::handle:horizontal {
    background: #00aaff; /* Parlak Neon Mavi */
    border: 1px solid #00aaff;
    width: 18px; /* Topuzun genişliği */
    height: 18px;
    margin: -7px 0; /* Ortalamak için */
    border-radius: 9px; /* Yuvarlak olması için */
}

/* 3. Dolan Kısım (Solda kalan alan) */
QSlider::sub-page:horizontal {
    background: #005bb5; /* Butonlarla aynı mavi */
    border-radius: 4px;
}</string>
